class Loop214
{
	public static void main(String args[])
	{
		int i,j;
		for(i=1;i<=5;i++)
		{
			for(int k=5;k>=i;k--)
			{
			System.out.print(" ");				
			}
			for(j=1;j<=i;j++)
			{
				if(j==1||i==j||i==5)
				{
				System.out.print(j+" ");					
				}
				else
				{
				System.out.print("  ");
					
				}
			}
		System.out.println("");
		}
	}
}